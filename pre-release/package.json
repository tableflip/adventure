{
  "name": "adventure-pre-release",
  "version": "0.0.0",
  "description": "pre-release service for TABLEFLIP projects",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "start": "micro",
    "watch": "npm-run-all --parallel watch:*",
    "watch:server": "nodemon --watch . --ext js,json,html --ignore node_modules --exec micro",
    "watch:browser": "browser-sync start --port 4000 --proxy localhost:3000 --files . --reload-delay 500"
  },
  "keywords": [
    "TABLEFLIP",
    "micro",
    "adventure",
    "pre-release"
  ],
  "author": "TABLEFLIP",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "browser-sync": "^2.18.12",
    "faker": "^4.1.0",
    "form-urlencoded": "^1.5.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "test-listen": "^1.0.2"
  },
  "dependencies": {
    "joi-browser": "^10.0.6",
    "micro": "^7.3.3",
    "urlencoded-body-parser": "^2.0.1"
  }
}
